#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
  cin.sync_with_stdio(0);
  cin.tie(0);
  cout.tie(0);
// freopen("input.txt","r",stdin);
  ll t, ans, n, x, y;
  cin >> t;
  while (t--)

  {
    cin >> n;
    ans = 1;
    ans = ((n - 1) * n) / 2;
    while (n)
    {
      ans = ans * n;
      n--;
    }
    cout << ans << "\n";
  }
